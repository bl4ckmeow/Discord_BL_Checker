{
  "name": "discord-blacklist-bot",
  "version": "1.0.0",
  "description": "Discord bot for managing trading blacklists with MySQL database",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:integration": "jest --config src/__tests__/integration/jest.integration.config.js",
    "test:integration:watch": "jest --config src/__tests__/integration/jest.integration.config.js --watch",
    "test:all": "npm run test && npm run test:integration",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "db:init": "ts-node src/database/init.ts"
  },
  "keywords": ["discord", "bot", "blacklist", "trading", "mysql"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^14.14.1",
    "mysql2": "^3.6.5",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}